{
	"name": "Awful Images",
	"version": "0.0.9",
	"description": "Replace [img-whatever] tags with appropriate [img] tags on the Someting Is Awful on the Internet dot com forums.",
	"icons": {
		"16": "img/icon.png",
		"48": "img/48.png",
		"128": "img/128.png"
	},
	"homepage_url": "http://github.com/rdhammond/Awful-Images.git",
	"permissions": [
		"http://forums.somethingawful.com/new*",
		"https://forums.somethingawful.com/new*",
		"storage"
	],
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "Awful Images"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [{ 
		"matches": ["http://forums.somethingawful.com/new*", "https://forums.somethingawful.com/new*"],
		"js": ["jquery-2.0.3.min.js", "main.js"]
	}],
	"web_accessible_resources": [
		"jquery-2.0.3.min.map"
	],
	"manifest_version": 2,
	"options_page": "options.html"
}
